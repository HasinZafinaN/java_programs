import java.util.*;
public class Main{
 public static void main(String []args){
      Scanner sc=new Scanner(System.in);
      int num=sc.nextInt();
      int ans=num;
      int sum=0;
      int count =0;
      while(ans>0){
         ans=ans/10;
        count++;
     }
     ans=num;
     while(ans>0){
         int r=ans%10;
         ans=ans/10;
         sum=sum+(int)(Math.pow(r,count));
     }
     System.out.println((sum==num)?"amstrong":"not amstrong");
 }
}
